<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape android:shape="oval">
        <corners android:radius="20dip" />

        <stroke android:width="1dip" android:color="@color/BurlyWood" />

        <gradient android:angle="-90" android:endColor="@color/Chocolate" android:startColor="@color/Chocolate" />
    </shape></item>
    <item><shape android:shape="oval">
        <corners android:radius="20dip" />

        <stroke android:width="1dip" android:color="@color/OrangeRed" />

        <gradient android:angle="-90" android:endColor="@color/DarkOrange" android:startColor="#fbbc05" />
    </shape></item>

</selector>